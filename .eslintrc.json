{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "semi": 1,
    "no-console": 1,
    "no-unused-vars": 1,
    "no-console": 1,
    "no-debugger": 1,
    "indent": [1, 2, {"SwitchCase": 1}],
    "linebreak-style": [1, "unix"]
  }
}
